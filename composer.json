{
    "name": "alg/lasttopicsajax",
    "type": "phpbb-extension",
    "description": "Adds block with some last topics sorted by columns",
    "homepage": "https://github.com/alg5/lasttopicsajax",
    "version": "1.0.0",
    "time": "2014-10-13",
    "license": "GPL-2.0",
    "authors": [
        {
            "name": "Alg",
            "homepage": "http://www.phpbbguru.net",
            "role": "Developer"
        }
       
    ],
    "require": {
        "php": ">=5.4.2"
    },
	"require-dev": {
      "phpbb/epv": "dev-master"
   },
	"extra": {
        "display-name": "Last Topics from any forum's page",
        "soft-require": {
           "phpbb/phpbb": "3.2.*@dev"
        },
        "version-check": {
            "host": "alg5.github.io",
            "directory": "/versions",
            "filename": "lasttopicsajax.json"
        }
    }
}